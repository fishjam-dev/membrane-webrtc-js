  "devDependencies": {
    "@playwright/test": "^1.39.0",
    "@types/events": "^3.0.0",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "prettier": "3.0.3",
    "rimraf": "^3.0.2",
    "testcontainers": "^10.2.2",
    "typedoc": "^0.23.0",
    "typescript": "^4.9.4"
